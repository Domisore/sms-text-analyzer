{
  "expo": {
    "jsEngine": "hermes",
    "name": "TextBud",
    "slug": "textile",
    "version": "2.4.1",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "package": "com.textbud.app",
      "icon": "./assets/appicons/playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicons/playstore.png",
        "backgroundColor": "#111827"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png"
      },
      "permissions": [
        "android.permission.READ_SMS",
        "android.permission.RECEIVE_SMS",
        "android.permission.SEND_SMS",
        "android.permission.READ_PHONE_STATE",
        "android.permission.WRITE_SMS",
        "android.permission.BROADCAST_SMS",
        "android.permission.RECEIVE_MMS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.textbud.app",
      "icon": "./assets/appicons/appstore.png",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "expo-document-picker",
      "expo-file-system",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "expo-font",
      "./plugins/withSMSPermissions.js",
      "react-native-iap"
    ],
    "extra": {
      "eas": {
        "projectId": "a34d186d-ecbe-4d6e-8570-12dce188ad34"
      }
    }
  }
}
